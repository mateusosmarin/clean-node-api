    "git-commit-msg-linter": "^3.2.6",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.1",
    "supertest": "^6.1.5",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
